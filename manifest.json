{
  "manifest_version": 3,
  "name": "RemoveWatchLater",
  "version": "1.0",
  "description": "Delete videos from your watch later playlist",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "host_permissions": [
    "*://www.youtube.com/*"
  ]
}
